  "license": "MIT",
  "repository": "https://github.com/tomzhu1024/webpack-boilerplate",
  "private": true,
  "scripts": {
    "prepare": "npx husky install",
    "start": "npx cross-env NODE_ENV=development TS_NODE_PROJECT=build/tsconfig.json npx ts-node build/scripts/start.ts",
    "build": "npx cross-env NODE_ENV=production TS_NODE_PROJECT=build/tsconfig.json npx ts-node build/scripts/build.ts",
    "eslint": "npx eslint ./**/*.{js,jsx,ts,tsx}",
    "eslint-fix": "npx eslint --fix ./**/*.{js,jsx,ts,tsx}",
    "stylelint": "npx stylelint ./**/*.{css,sass,scss,less}",
    "stylelint-fix": "npx stylelint --fix ./**/*.{css,sass,scss,less}",
    "prettier-check": "npx prettier --check ./**/*.{js,jsx,ts,tsx}",
    "prettier-write": "npx prettier --write ./**/*.{js,jsx,ts,tsx}"
  },
